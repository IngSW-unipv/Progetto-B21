                      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_-zIPpd17EeuHgZs72is1JA" name="makePresa" specification="_-xeCsN17EeuHgZs72is1JA">
                        <ownedParameter xmi:id="_-zIPpt17EeuHgZs72is1JA" name="cardsOnBoard" type="_-wJNAN17EeuHgZs72is1JA"/>
                        <ownedParameter xmi:id="_-zIPp917EeuHgZs72is1JA" direction="return">
                          <name xsi:nil="true"/>
                          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                        </ownedParameter>
                        <language>java</language>
                        <body>if (cardsOnBoard.isEmpty()) {

			setCardPlayed(getDeck().get(0));
			getCardsListTemp().add(getDeck().get(0));

			return 1;
		}

		else {

			for (Card deck : getDeck()) {

				int counter = 0;
				ArrayList&lt;Card> cardList = new ArrayList&lt;Card>();

				for (Card table : cardsOnBoard) {

					counter += table.getValue();
					cardList.add(table);

					if (counter == deck.getValue() &amp;&amp; cardList.size() != 1) {

						getCardsListTemp().addAll(cardList);
						getCardsListTemp().add(deck);
						setCardPlayed(deck);

						return 1;
					}

					else {

						if (deck.getValue() == table.getValue()) {

							getCardsListTemp().add(table);
							getCardsListTemp().add(deck);
							setCardPlayed(deck);

							return 1;
						}
					}
				}
			}
		}
		return 0;
	</body>
                      </ownedBehavior>
