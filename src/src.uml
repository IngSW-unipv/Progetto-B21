                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_-zFMQd17EeuHgZs72is1JA" name="counter" specification="_-prYoN17EeuHgZs72is1JA">
                    <ownedParameter xmi:id="_-zFMQt17EeuHgZs72is1JA" name="t">
                      <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                    </ownedParameter>
                    <language>java</language>
                    <body>if (t == seconds) {
			for (int i = t; i >= 0; i--) {
				seconds = i;
				controller.personalAdvisor(String.valueOf(i));
				wait(1000);
			}
		} else {
			for (int i = t; i >= 0; i--) {
				controller.gameAdvisor(String.valueOf(i));
				wait(1000);
			}
		}
	</body>
                  </ownedBehavior>
