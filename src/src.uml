                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_-zDXE917EeuHgZs72is1JA" name="singleplayer" specification="_-pkq8917EeuHgZs72is1JA">
                    <language>java</language>
                    <body>player = new HumanPlayer(nickname);
		ArrayList&lt;Player> p = new ArrayList&lt;Player>();
		p.add(player);
		game = new Game(p);
		game.start();
		this.threads = new SingleplayerThread[4];
		for (int i = 0; i &lt; 4; i++) {
			threads[i] = new SingleplayerThread(game, game.getPlayers().get(i), controller);
		}
		this.playerThread = threads[0];
		for (int i = 0; i &lt; 4; i++) {
			threads[i].start();
		}
	</body>
                  </ownedBehavior>
