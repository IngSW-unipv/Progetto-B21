                  <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_-zGabt17EeuHgZs72is1JA" name="initialize" specification="_-p40AN17EeuHgZs72is1JA">
                    <language>java</language>
                    <body>if (players.size() &lt; 4) {&#xD;
			for (int i = 4-players.size(); i > 0; i--) {&#xD;
				this.players.add(new BotPlayer(((i%2==0)?&quot;botA&quot;:&quot;botB&quot;)));&#xD;
			}&#xD;
		}&#xD;
		&#xD;
		cardsOnBoard = new ArrayList&lt;Card>();&#xD;
		teams = new ArrayList&lt;Team>();&#xD;
		deck = new ArrayList&lt;Card>();&#xD;
		shuffledDeck = new ArrayList&lt;Card>();&#xD;
		turn = 0;&#xD;
		&#xD;
	</body>
